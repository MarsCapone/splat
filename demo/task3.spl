function list t3 (list splat) {
    if splat == [] {
        []
    } else {
        let [v1 = (as_num (head (split (head splat))))] {
            let [v2 = (as_num (head (tail (split (head splat)))))] {
                let [s = (showln v1 + (v2*3)) ] {
                    v1 + (v2*3) :: (t3 #(tail splat))
                }
            }
        }
    }
} #stdin
